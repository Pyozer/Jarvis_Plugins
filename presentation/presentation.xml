<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="rulepresentation" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
  <rule id="rulepresentation" scope="public">
    <example>Jarvis, présente toi</example>
    <tag>out.action=new Object(); </tag>

    <item>Jarvis</item>

    <one-of>
      <item>présente toi<tag>out.action.test = "Bonjour"</tag></item>
      <item>tu peux te présenter<tag>out.action.test = "Bonjour"</tag></item>
      <item>t'es qui<tag>out.action.test = "Bonjour"</tag></item>
      <item>tu es qui<tag>out.action.test = "Bonjour"</tag></item>
      <item>tu est qui<tag>out.action.test = "Bonjour"</tag></item>
      <item>dit qui tu es<tag>out.action.test = "Bonjour"</tag></item>
      <item>dit qui tu est<tag>out.action.test = "Bonjour"</tag></item>
      <item>qui est tu<tag>out.action.test = "Bonjour"</tag></item>
      <item>qui es tu<tag>out.action.test = "Bonjour"</tag></item>
      <item>tu est quoi<tag>out.action.test = "Bonjour"</tag></item>
      <item>tu es quoi<tag>out.action.test = "Bonjour"</tag></item>
      <item>t'es quoi<tag>out.action.test = "Bonjour"</tag></item>
    </one-of>

    <tag>out.action._attributes.uri="http://127.0.0.1:8080/sarah/presentation";</tag>
  </rule> 
</grammar>